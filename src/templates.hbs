{{#each this}}
<div class="photo-card">
    <div class="image-card">
    <a href="{{largeImageURL}}">
        <img  class="gallery_img"
    src="{{webformatURL}}" width="300px" height="200px" alt="{{tags}}" loading="lazy" />
    </a>
    </div>
    <div class="info">
        <p class="info-item">
            <b>Likes</b>
            {{ likes }}
        </p>
        <p class="info-item">
            <b>Views</b>
            {{views}}
        </p>
        <p class="info-item">
            <b>Comments</b>
            {{ comments }}
        </p>
        <p class="info-item">
            <b>Downloads</b>
            {{ downloads }}
        </p>
    </div>
    
</div>
{{/each}}